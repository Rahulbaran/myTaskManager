{
  "extends": "eslint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "spaced-comment": "warn",
    "no-unused-vars": "error",
    "object-shorthand": "off",
    "no-underscore-dangle": "warn",
    "prefer-destructuring": "error",
    "prefer-object-spread": "off",
    "no-return-await": "off",
    "consistent-return": "off",
    "no-process-exit": "off",
    "no-process-reassign": "off",
    "func-names": "off",
    "prefer-spread": "warn",
    "strict": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  }
}
